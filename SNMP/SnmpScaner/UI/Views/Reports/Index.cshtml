@model IEnumerable<DomainModel.EfModels.Report>

@{
    ViewBag.Title = "Index";
}

<h2>Список отчетов</h2>
<hr />
<div class="well">
    @foreach (var item in Model)
    {
        <div>
            @Html.ActionLink(item.ReportName, "ShowReport", new { id = item.IdReport })
        </div>
    }
</div>
